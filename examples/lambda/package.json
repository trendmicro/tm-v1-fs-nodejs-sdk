{
  "name": "cloudone-antimalware-aws-lambda-examples",
  "version": "1.0.0",
  "description": "Cloud One Antimalware API library examples for AWS Lambda in JavaScript",
  "license": "MIT",
  "author": "Trend Micro Inc",
  "main": "index.js",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "esbuild src/$SOURCE --bundle --platform=node --packages=external --outfile=dist/index.js",
    "postbuild": "cp package.json dist && cd dist && npm install --omit=dev && 7z a index.zip index.js package.json node_modules"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.299.0",
    "@tsconfig/node16": "^16.1.1",
    "@types/aws-lambda": "^8.10.109",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "aws-lambda": "^1.0.7",
    "esbuild": "^0.16.17",
    "eslint": "^8.48.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "file-security-sdk": "^1.0.0"
  }
}
