{
  "name": "cloudone-antimalware-cli-client-examples",
  "version": "1.0.0",
  "description": "Cloud One Antimalware API library examples for CLI client in JavaScript",
  "license": "MIT",
  "author": "Trend Micro Inc",
  "main": "index.js",
  "engines": {
    "node": ">=16.20.1"
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "npm run lint && esbuild src/$SOURCE --bundle --platform=node --packages=external --outfile=dist/index.js",
    "postbuild": "cp package.json dist && cp -R node_modules dist",
    "client": "cd dist && node index.js",
    "lint": "eslint src --ext .ts",
    "fix": "eslint src --ext .ts --fix"
  },
  "devDependencies": {
    "@tsconfig/node16": "^16.1.1",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "esbuild": "^0.16.17",
    "eslint": "^8.46.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "file-security-sdk": "^1.0.0"
  }
}
